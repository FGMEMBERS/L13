  <animation>
    <type>pick</type>
    <object-name>canopyfull</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
          <condition><and>
                    <greater-than>
                <property>controls/canopy/lock-left</property>
                    <value>0.8</value>
                </greater-than>
                <less-than>
                <property>controls/canopy/lock-right</property>
                <value>0.5</value>
            </less-than>
        </and>   
                </condition>
        <command>nasal</command>
        <script>doors.canopy.toggle();</script>
      </binding>
      <binding>
          <condition>
              <and>
                    <greater-than>
                <property>controls/canopy/lock-left</property>
                    <value>0.8</value>
                </greater-than>
                <greater-than>
                <property>controls/canopy/lock-right</property>
                <value>0.5</value>
            </greater-than>
        </and>
                    
                </condition>
        <command>nasal</command>
        <script>doors.canopy_jettison.toggle();</script>
      </binding>
    </action>
  </animation>
  
